
#map_canvas{
          width: 100%;
          height: 400px;
          overflow: hidden;
}
#marker_button{
          position: absolute;
          z-index: 100;
          margin:100px 0 0 0;
}
.sidebar_button {
          background-image: url("../images/ControlButton.png");
          cursor: pointer;
          display: block;
          float: left;
          height: 31px;
          margin: 0 0 3px 3px;
          text-indent: -9999px;
          width: 31px;
}
#sidebar_map {
          padding: 5px;
          position: absolute;
          top: 0;
          width: 37px;
          z-index: 1002;
          background-image: none;

}
#bg_sidebar{
          padding:5px;
          top: 0;
          background: #eee;
          width: 37px;
          height: 100%;
          position: absolute;
          opacity:0.5;
}
#map_goo{
          position: relative;
          width:96%;
          height: 400px;
          margin:3px 0 0 0;
          padding:0;
          overflow: hidden;
          border:1px solid #6699ff;
}
#map_goo a{
          cursor: pointer;
}